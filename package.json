{
  "name": "quoteosch",
  "version": "1.0.3",
  "description": "An API to spread the eternal wisdom of BME lecturers",
  "author": "Barni <szocsbarnabas8@gmail.com> (https://github.com/BrNi05)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrNi05/Quoteosch.git}"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "rm -f tsconfig.build.tsbuildinfo && nest build",
    "start:prod": "node dist/main",
    "start:dev": "rm -f tsconfig.build.tsbuildinfo && nest start --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js",
    "db:generate": "npm run typeorm -- migration:generate src/migrations/InitTables -d src/data-source.ts",
    "db:init": "npm run typeorm -- migration:run -d dist/data-source.js",
    "db:init:prod": "npm run typeorm:prod -- migration:run -d dist/data-source.js",
    "docker:build": "npm run build && docker build -t quoteosch .",
    "docker:init": "docker compose run --rm quoteosch-api-init",
    "docker:up": "docker compose up -d quoteosch-api",
    "docker:down": "docker compose down",
    "docker:prune": "docker system prune -a -f",
    "lint": "eslint . --ext .ts --cache",
    "format": "prettier --write \"**/*.{ts,json,md}\" --log-level silent",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "postinstall": "node -e \"if (process.env.NODE_ENV !== 'production') require('child_process').execSync('git config core.hooksPath .husky')\"",
    "unsafe": "git config --unset core.hooksPath",
    "release:docker": "npm run build && node scripts/release-docker.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "joi": "^18.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^5.0.6",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
